<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="9%p"
    android:keyWidth="25%p"
    android:verticalGap="10%p"
    android:horizontalGap="10%p" >

    
    <Row android:verticalGap="30px" >
        <Key
            android:codes="250"
            android:horizontalGap="10%p"
            android:keyEdgeFlags="left"
			android:keyIcon="@drawable/btn_backward"/>
        
        <Key
            android:codes="251"
            android:horizontalGap="5%p"
			android:keyIcon="@drawable/btn_forward" />
        <Key
            android:codes="252"
            android:horizontalGap="5%p"
			android:keyIcon="@drawable/btn_exit" />
        <Key
            android:horizontalGap="5%p"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row>
        <Key
            android:codes="253"
            android:horizontalGap="10%p"
            android:keyEdgeFlags="left"
			android:keyIcon="@drawable/btn_stop" />
        <Key
            android:codes="254"
            android:horizontalGap="5%p"
			android:keyIcon="@drawable/btn_play_circle"/>
        <Key
            android:codes="255"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="right"
			android:keyIcon="@drawable/btn_pause" />
    </Row>

</Keyboard>