            android:keyEdgeFlags="right"
            android:keyLabel="P"
            android:popupCharacters="p"
            android:popupKeyboard="@xml/popup" />
    </Row>
    <Row>
        <Key
            android:codes="65"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="A"
            android:popupCharacters="a"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="83"
