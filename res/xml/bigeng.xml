            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="71"
            android:keyLabel="G"
            android:popupCharacters="g"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="72"
            android:keyLabel="H"
            android:popupCharacters="h"
            android:popupKeyboard="@xml/popup" />
