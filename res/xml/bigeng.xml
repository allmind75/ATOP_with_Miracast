            android:codes="74"
            android:keyLabel="J"
            android:popupCharacters="j"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="75"
            android:keyLabel="K"
            android:popupCharacters="k"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="76"
            android:keyEdgeFlags="right"
            android:keyLabel="L"
            android:popupCharacters="l"
            android:popupKeyboard="@xml/popup" />
    </Row>
    <Row>
        <Key
            android:codes="10"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_up"
            android:keyWidth="15%p" />
        <Key
            android:codes="90"
            android:keyLabel="Z"
            android:popupCharacters="z"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="88"
            android:keyLabel="X"
            android:popupCharacters="x"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="67"
            android:keyLabel="C"
            android:popupCharacters="c"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="86"
            android:keyLabel="V"
            android:popupCharacters="v"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="66"
            android:keyLabel="B"
            android:popupCharacters="b"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="78"
            android:keyLabel="N"
            android:popupCharacters="n"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="77"
            android:keyLabel="M"
            android:popupCharacters="m"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="11"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyWidth="15%p" />
    </Row>
    <Row>
        <Key
            android:codes="12"
            android:keyEdgeFlags="left"
            android:keyLabel="기호"
            android:keyWidth="15%p" />
        <Key
