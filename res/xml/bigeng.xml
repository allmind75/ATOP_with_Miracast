<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="8.5%p"
    android:keyWidth="10%p"
    android:verticalGap="5px" >

    <Row>
        <Key
            android:codes="81"
            android:keyEdgeFlags="left"
            android:keyLabel="Q"
            android:popupCharacters="q"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="87"
            android:keyLabel="W"
            android:popupCharacters="w"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="69"
            android:keyLabel="E"
            android:popupCharacters="e"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="82"
            android:keyLabel="R"
            android:popupCharacters="r"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="84"
            android:keyLabel="T"
            android:popupCharacters="t"
            android:popupKeyboard="@xml/popup" />
        <Key
            android:codes="89"
            android:keyLabel="Y"
